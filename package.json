{
  "name": "ask-docs-widget",
  "scripts": {
    "start:client": "cd packages/client && npm start",
    "start:server": "cd packages/server && npm start",
    "start:all": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "ingest": "cd packages/scripts && npm run ingest"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
